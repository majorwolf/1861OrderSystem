# Production environment variables
NODE_ENV=production

# Database configuration
# These will be overridden by the docker-compose.yml environment section
# but are included here as a reference
DATABASE_URL=postgres://postgres:postgres@db:5432/orderingsystem
PGHOST=db
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=orderingsystem
PGPORT=5432

# Session configuration
SESSION_SECRET=change_this_to_a_secure_random_string_in_production

# Server configuration
PORT=5000